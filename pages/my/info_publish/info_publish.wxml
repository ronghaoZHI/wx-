      </view>
      <!-- 内容 -->
      <view class='tab_bd'>
        <!-- 最新发布 -->
        <view class="ul active" >
          <view class='li' wx:for="{{list}}">
            <view class='li_hd'>
              <view class='li_hd_hd'>
