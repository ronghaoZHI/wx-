<view class="page page_discovery" xmlns:wx="http://www.w3.org/1999/xhtml">
  <view class="page__bd">  
    <!-- 选项卡 -->
    <view class='section_tab style1'>
      <!-- 头部 -->
      <view class='tab_hd'>
        <navigator url="../info_collect/info_collect" class='tab_hd_item' hover-class='none' hover-stay-time='{{navigator_time}}'>
          发布的信息
        </navigator>
        <navigator url="../info_publish/info_publish" class='tab_hd_item  active' hover-class='none' hover-stay-time='{{navigator_time}}'>
          收藏的信息
        </navigator>
        <!-- <navigator url="../info_interest/info_interest" class='tab_hd_item' hover-class='none'  hover-stay-time='{{navigator_time}}'>
